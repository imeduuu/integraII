// Prisma schema for PostgreSQL
// Puedes definir tus modelos aqu√≠

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
}
